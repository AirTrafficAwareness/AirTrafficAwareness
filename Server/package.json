{
  "name": "server",
  "version": "1.0.0",
  "description": "TCAS Server",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/server.ts",
    "lint": "eslint ./src/**/*.ts",
    "prod": "npm run build && npm run start",
    "start": "node ./build/src/server.js",
    "test": "nyc mocha"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.3",
    "open": "^7.0.3",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/ws": "^7.2.3",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
